{
  "name": "wetax",
  "version": "1.0.0",
  "main": "expo/AppEntry.js",
  "scripts": {
    "start": "expo start -c",
    "start:lan": "expo start --lan --clear",
    "start:tunnel": "expo start --tunnel --clear",
    "start:local": "$env:EXPO_NO_UPDATES='1'; expo start --lan --clear",
    "start:ios": "expo start --lan --clear",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "build:android": "NODE_ENV=production eas build --profile production --platform android",
    "build:ios": "NODE_ENV=production eas build --profile production --platform ios",
    "build:ios:local": "NODE_ENV=production eas build --profile production --platform ios --local",
    "build:ios:development": "eas build --profile development --platform ios",
    "build:ios:development:local": "NODE_ENV=production eas build --profile development --platform ios --local",
    "build:ios:development-simulator:local": "eas build --profile development-simulator --platform ios --local",
    "build:ios:adeola": "eas build --profile development-adeola --platform ios",
    "start:adeola": "expo start --dev-client",
    "update:adeola": "eas update --channel dev-adeola",
    "eas:update:production": "eas update --branch production --message 'fix'",
    "web": "expo start --web",
    "openapi": "openapi --input http://localhost:3000/swagger.json --output ./src/openapi",
    "fix": "npm run expo -- fix"
  },
