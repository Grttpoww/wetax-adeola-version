	<xs:import namespace="http://www.ech.ch/xmlns/eCH-0046-f/5" schemaLocation="./eCH-0046-5-0f.xsd"/>
	<xs:import namespace="http://www.ech.ch/xmlns/eCH-0119/4" schemaLocation="./eCH-0119-4-0-0.xsd"/>
	<!-- Header -->
	<xs:element name="headerExtension" type="zh:headerExtensionType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Erweiterungs-Element für den header</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="headerExtensionType">
		<xs:sequence>
			<xs:element name="hiddenData" type="zh:hiddenDataType" minOccurs="0"/>
			<xs:element name="approvalReceipt" type="zh:approvalReceiptType" minOccurs="0"/>
			<xs:element name="sourceSystem" type="zh:systemType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="documentList" type="zh:documentListType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="reducedDispatch" type="xs:boolean" minOccurs="0"/>
			<xs:element name="versionFK" type="xs:string" minOccurs="0"/>
			<xs:element name="clientPasswordProtection" type="xs:boolean" minOccurs="0"/>
			<xs:element name="scanning" type="zh:scanningType" minOccurs="0"/>
			<xs:element name="triageKind" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="K">
							<xs:annotation>
								<xs:documentation xml:lang="de">Kantonales Steueramt</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="G">
							<xs:annotation>
								<xs:documentation xml:lang="de">Gemeinde</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="taxGroup" type="xs:string" minOccurs="0"/>
			<xs:element name="limitedTaxLiability" type="xs:boolean" minOccurs="0"/>
			<xs:element name="businessPortal" type="xs:boolean" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="hiddenDataType">
		<xs:sequence>
			<xs:element name="differenceInPropertyRevenueP1" type="ssk:moneyType1" minOccurs="0"/>
			<xs:element name="differenceInPropertyRevenueP2" type="ssk:moneyType1" minOccurs="0"/>
			<xs:element name="selfEmploymentP1" type="xs:boolean" minOccurs="0"/>
			<xs:element name="noSelfEmploymentP1" type="xs:boolean" minOccurs="0"/>
			<xs:element name="selfEmploymentP2" type="xs:boolean" minOccurs="0"/>
			<xs:element name="noSelfEmploymentP2" type="xs:boolean" minOccurs="0"/>
			<xs:element name="relevantCooperation" type="xs:boolean" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="approvalReceiptType">
		<xs:sequence>
			<xs:element name="roundedTaxableIncome" type="ssk:taxAmountType" minOccurs="0"/>
			<xs:element name="roundedRatedeterminingIncome" type="ssk:taxAmountType" minOccurs="0"/>
			<xs:element name="roundedTaxableAsset" type="ssk:moneyType1" minOccurs="0"/>
			<xs:element name="roundedRatedeterminingAsset" type="ssk:moneyType1" minOccurs="0"/>
			<xs:element name="taxReturnKind" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="0">
							<xs:annotation>
								<xs:documentation xml:lang="de">ETC – "Anderer"</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="1">
							<xs:annotation>
								<xs:documentation xml:lang="de">UWA – "Wegzug in das Ausland"</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="2">
							<xs:annotation>
								<xs:documentation xml:lang="de">TF – "Todesfall"</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="3">
							<xs:annotation>
								<xs:documentation xml:lang="de">HPF – "Hinterbliebener Pflichtiger"</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="4">
							<xs:annotation>
								<xs:documentation xml:lang="de">HPA – "Hinterbliebener Partner"</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="5">
							<xs:annotation>
								<xs:documentation xml:lang="de">NVA - "Nachträgliche Veranlagung"</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="6">
							<xs:annotation>
								<xs:documentation xml:lang="de">EVA - "Ergänzende Veranlagung"</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="7">
							<xs:annotation>
								<xs:documentation xml:lang="de">TOL - "Tolerierte VST zur Quellensteuer"</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="systemType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Detaillierte Angaben über das Ursprungsystem des XML</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="system" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Bezeichnung des Systems</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="version" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Version des Release des Erstellungssystem, mit der das XML erstellt wurde</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="operatingSystem" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Betriebssystem, auf welchem das Erstellungssystem oder Browser installiert ist</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="browser" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Browser, mit welchem das Online-Erstellungssystem verwendet wurde</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="date" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Datum, an dem das XML mit diesem System erstellt/verändert wurde</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="documentListType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Detaillierte Angaben über die Belege, die der Steuerpflichtige zusammen mit seiner Steuererklärung einreichen muss.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="documentType" type="zh:documentTypeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Belegart, welcher vom Steuerpflichtigen eingereicht werden muss</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="documentDeliveryMethod" type="zh:documentDeliveryMethodType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Art der Zustellung des Belegs.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="documentDescription" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Textliche Beschreibung des Belegs, der vom Steuerpflichtigen eingereicht werden muss. Wird auf der Freigabequittung aufgedruckt</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
